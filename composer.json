{
  "name": "acquia/blt-project",
  "description": "Project template for Drupal 8 sites built with BLT.",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/SU-SWS/stanford_basic"
    },
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.8.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "kenwheeler/slick",
        "version": "1.8.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/kenwheeler/slick/archive/1.8.0.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "dinbror/blazy",
        "version": "1.8.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    }
  ],
  "require": {
    "acquia/blt": "9.x-dev",
    "beleneglorion/php-bugherd-api": "dev-master#f0fce97042bd05bc5560fbf37051cd74f802cea1",
    "composer/installers": "^1.4",
    "consolidation/robo": "~1.2.4",
    "cweagans/composer-patches": "^1.6",
    "davidbarratt/custom-installer": "~1.0",
    "dinbror/blazy": "^1.8",
    "drupal/acquia_connector": "~1.13",
    "drupal/acquia_purge": "^1.0@beta",
    "drupal/address": "~1.2",
    "drupal/admin_login_path": "^1.0",
    "drupal/admin_toolbar": "~1.20",
    "drupal/allowed_formats": "~1.1",
    "drupal/auto_entitylabel": "2.x-dev",
    "drupal/block_class": "dev-1.x#0a3996529627643fd3a326b0f567f4b1f5056960",
    "drupal/block_content_permissions": "^1.6",
    "drupal/block_field": "^1.0@alpha",
    "drupal/bricks": "dev-1.x",
    "drupal/bugherdapi": "^1.1",
    "drupal/chosen": "~2.4",
    "drupal/color_field": "~2.0-rc2",
    "drupal/components": "~1.0",
    "drupal/config_ignore": "^2.1",
    "drupal/config_import_de": "^1.0@alpha",
    "drupal/config_installer": "^1.8",
    "drupal/config_readonly": "dev-1.x",
    "drupal/config_update": "dev-1.x#5785588167279912d1e1f3a936fbf4a306265bd6",
    "drupal/core": "~8.6.0-beta2",
    "drupal/default_content": "^1.0@alpha",
    "drupal/draggableviews": "dev-1.x",
    "drupal/dropzonejs": "~2.0@alpha",
    "drupal/ds": "dev-3.x#401e05508844187dc31ad6dc9433cb15237d4c49",
    "drupal/eck": "~1.0@alpha",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/embed": "~1.0",
    "drupal/encrypt": "^3.0@alpha",
    "drupal/entity_browser": "2.x-dev",
    "drupal/entity_embed": "~1.0@beta",
    "drupal/entity_reference_exposed_filters": "^1.0@alpha",
    "drupal/entity_reference_revisions": "^1.3",
    "drupal/entityqueue": "^1.0@alpha",
    "drupal/environment_indicator": "^3.3",
    "drupal/extlink": "1.x-dev",
    "drupal/field_formatter_class": "^1.1",
    "drupal/field_group": "~1.0@RC",
    "drupal/field_permissions": "^1.0@beta",
    "drupal/focal_point": "^1.0@beta",
    "drupal/fontawesome": "^2.4",
    "drupal/google_analytics": "~2.1",
    "drupal/honeypot": "^1.27",
    "drupal/inline_entity_form": "~1.0@beta",
    "drupal/jira_rest": "^3.0",
    "drupal/key": "^1.7",
    "drupal/linkit": "~5.0-beta6",
    "drupal/masquerade": "^2.0@beta",
    "drupal/material_admin": "^1.0@alpha",
    "drupal/menu_admin_per_menu": "^1.0",
    "drupal/menu_block": "^1.5",
    "drupal/menu_link_weight": "^1.0@beta",
    "drupal/menu_position": "1.x-dev",
    "drupal/metatag": "~1.3",
    "drupal/migrate_plus": "dev-4.x#88b0d1433408451e0d6815e365a2778a91bc498d",
    "drupal/migrate_tools": "~4.0",
    "drupal/paragraphs": "~1.2",
    "drupal/path_redirect_import": "^1.0@beta",
    "drupal/pathauto": "~1.0",
    "drupal/porterstemmer": "^1.0",
    "drupal/rabbit_hole": "~1.0@beta",
    "drupal/real_aes": "^2.1",
    "drupal/redirect": "~1.0-beta1",
    "drupal/role_delegation": "^1.0@alpha",
    "drupal/role_theme_switcher": "~1.0",
    "drupal/search_api": "^1.9",
    "drupal/shield": "~1.1",
    "drupal/simplesamlphp_auth": "^3.0",
    "drupal/slick_paragraphs": "^1.0@RC",
    "drupal/smart_trim": "^1.1",
    "drupal/taxonomy_menu_ui": "^1.0@beta",
    "drupal/ui_patterns": "~1.0@RC",
    "drupal/video_embed_field": "~2.0-alpha1",
    "drupal/viewfield": "^3.0@alpha",
    "drupal/views_block_filter_block": "^1.0@beta",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/views_taxonomy_term_name_depth": "6.x-dev",
    "drupal/webform": "~5.0-beta18",
    "drupal/xmlsitemap": "dev-1.x",
    "drupal/yearonly": "^1.3",
    "drush/drush": "dev-master#3630f991b0d392a80f096e353b535d30526e37aa",
    "enyo/dropzone": "^5.1",
    "harvesthq/chosen": "^1.8",
    "kenwheeler/slick": "^1.8",
    "simplesamlphp/simplesamlphp": "~1.15.1",
    "su-sws/nobots": "dev-8.x-1.x",
    "su-sws/stanford_basic": "dev-8.x-2.x#144f6ec87e79b2e29f567e33aaf60894134f2af2",
    "su-sws/stanford_media": "dev-8.x-1.x"
  },
  "require-dev": {
    "drupal/asset_injector": "2.x-dev",
    "drupal/config_inspector": "^1.0@beta",
    "drupal/console": "^1.8",
    "drupal/devel": "^1.2",
    "drupal/devel_debug_log": "^1.0@beta",
    "drupal/search_kint": "1.0",
    "drupal/seckit": "^1.0@alpha",
    "jcalderonzumba/gastonjs": "^1.2",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.3",
    "symfony/phpunit-bridge": "*"
  },
  "extra": {
    "enable-patching": true,
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "custom-installer": {
      "docroot/modules/stanford/{$name}/": [
        "type:stanford-module"
      ],
      "docroot/themes/stanford/{$name}/": [
        "type:stanford-theme"
      ],
      "docroot/profiles/stanford/{$name}/": [
        "type:stanford-profile"
      ],
      "docroot/libraries/{$name}/": [
        "type:component",
        "ckeditor/ckeditor",
        "enyo/dropzone",
        "components/jquery"
      ],
      "vendor/{$vendor}/{$name}/": [
        "type:library"
      ]
    },
    "patches": {
      "drush/drush": {
        "partial-patch": "patches/contrib/drush-partial.patch"
      },
      "drupal/core": {
        "https://www.drupal.org/project/drupal/issues/564106": "https://www.drupal.org/files/issues/2018-06-27/views-more-link-token-replacement_564106_171.patch",
        "Simplify Field Samples": "patches/contrib/core-simplify-samples.patch"
      },
      "drupal/menu_block": {
        "https://www.drupal.org/project/menu_block/issues/2756675": "https://www.drupal.org/files/issues/2018-05-10/menu_block-follow-2756675-50.patch",
        "https://www.drupal.org/project/menu_block/issues/2809699": "https://www.drupal.org/files/issues/2018-05-07/menu_block-label_configuration-2809699-77.patch"
      },
      "drupal/migrate_plus": {
        "https://www.drupal.org/project/migrate_plus/issues/2837684": "https://www.drupal.org/files/issues/2837684-8-migrate-plus-xml-return-as-xml.patch",
        "https://www.drupal.org/project/migrate_plus/issues/2976091": "https://www.drupal.org/files/issues/2018-05-29/migrate_plus-fix_simple_xml_reduce_single_value-2976091-2.patch"
      },
      "drupal/views_taxonomy_term_name_depth": {
        "https://www.drupal.org/project/views_taxonomy_term_name_depth/issues/2877249": "https://www.drupal.org/files/issues/views_taxonomy_term_name_depth-allow_multiple-7.patch"
      },
      "drupal/ui_patterns": {
        "patches": "patches/contrib/ui_patterns_views-preview.patch"
      },
      "drupal/ds": {
        "https://www.drupal.org/project/ds/issues/2966959": "https://www.drupal.org/files/issues/2018-04-27/2966959-ds-namespace_form_for_layout_builder-4.patch",
        "https://www.drupal.org/project/ds/issues/2939322": "https://www.drupal.org/files/issues/2018-06-20/ds-content_entity-2939322-2.patch"
      },
      "drupal/key": {
        "https://www.drupal.org/project/key/issues/2980072": "https://www.drupal.org/files/issues/2018-06-18/key-encrypt-2980072-4.patch"
      },
      "drupal/role_theme_switcher": {
        "https://www.drupal.org/project/role_theme_switcher/issues/2980351": "https://www.drupal.org/files/issues/2018-06-18/role_theme_switcher-admin_theme-2980351-3.patch"
      },
      "drupal/config_update": {
        "https://www.drupal.org/project/config_update/issues/2974637": "https://www.drupal.org/files/issues/2018-06-11/drush9-support-2974637-24.patch"
      },
      "drupal/jira_rest": {
        "https://www.drupal.org/project/jira_rest/issues/2982892": "https://www.drupal.org/files/issues/2018-08-22/jira_rest-key_error-2982892-6.patch"
      },
      "drupal/entity_reference_revisions": {
        "Prevent recursion and unwanted entity saves": "patches/contrib/entity_reference_revisions-recursion-fix.patch"
      },
      "drupal/config_import_de": {
        "https://www.drupal.org/project/config_import_de/issues/2991762": "https://www.drupal.org/files/issues/2018-08-09/config_import_de-default-confg.patch"
      },
      "drupal/menu_link_weight": {
        "https://www.drupal.org/project/menu_link_weight/issues/2995896": "https://www.drupal.org/files/issues/2018-08-29/2995896-menu-admin-per-menu-support-fixes.patch"
      },
      "drupal/masquerade": {
        "https://www.drupal.org/project/masquerade/issues/2975124": "https://www.drupal.org/files/issues/2018-05-24/remove-hook-logout-but-clear-template-cache-2975124-1.patch"
      },
      "drupal/bricks": {
        "https://www.drupal.org/project/bricks/issues/3003176": "https://www.drupal.org/files/issues/2018-09-28/bricks-entity_usage-3003176.patch"
      }
    },
    "merge-plugin": {
      "require": [
        "blt/blt.required.json"
      ],
      "merge-extra": true,
      "merge-extra-deep": true,
      "merge-scripts": true,
      "replace": false,
      "ignore-duplicates": true
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\hs_basic_page\\": "docroot/modules/humsci/hs_basic_page/tests",
      "Drupal\\Tests\\hs_bugherd\\": "docroot/modules/humsci/hs_bugherd/tests",
      "Drupal\\Tests\\hs_courses_importer\\": "docroot/modules/humsci/hs_courses/modules/hs_courses_importer/tests",
      "Drupal\\Tests\\hs_field_helpers\\": "docroot/modules/humsci/hs_field_helpers/tests",
      "Drupal\\Tests\\hs_table_filter\\": "docroot/modules/humsci/hs_table_filter/tests"
    }
  },
  "scripts": {
    "post-drupal-scaffold-cmd": [
      "cd docroot && patch -p1 <../patches/htaccess-saml.patch",
      "patch -p1 < patches/htaccess-https.patch"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
