{
  "root": true,
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-restricted-globals": 0,
    "no-console": ["error", { "allow": ["error"] }],
    "no-continue": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "func-names": 0,
    "no-param-reassign": 0
  }
}
