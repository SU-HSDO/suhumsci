{
  "name": "humsci_basic",
  "version": "1.0.0",
  "description": "Humsci Basic",
  "main": "Gruntfile.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "backstop:init": "node ./backstop/generate-backstop.js",
    "backstop:reference": "backstop reference --config='./backstop/backstop.json'",
    "backstop:test": "backstop test --config='./backstop/backstop.json'",
    "build:sass": "grunt compile",
    "build:js": "webpack",
    "build": "npm run build:sass && npm run build:js",
    "lint:sass": "stylelint 'src/scss/**/*.scss' --config ../../../../.stylelintrc",
    "test:sass": "mocha 'tests/true-sass-spec.js'",
    "watch:sass": "grunt watch",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "watch:js": "webpack --watch",
    "watch": "npm run watch:sass & npm run watch:js",
    "test": "npm run lint:sass && npm run test:sass",
    "visreg:colorful": "percy snapshot tests/.snapshots-colorful.js",
    "visreg:traditional": "percy snapshot tests/.snapshots-traditional.js",
    "copy": "node ./tasks/copy.js"
  },
  "license": "MIT",
  "dependencies": {
    "bourbon": "^5.1.0",
    "decanter": "^6.0.0",
    "dotenv": "^8.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@percy/cli": "^1.10.1",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "backstopjs": "^6.0.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-webpack-plugin": "^3.0.1",
    "grunt": "^1.0.4",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-dart-sass": "^1.1.3",
    "grunt-postcss": "^0.9.0",
    "grunt-run": "^0.8.1",
    "mocha": "^6.2.2",
    "sass": "^1.23.7",
    "sass-true": "^5.0.0",
    "shelljs": "^0.8.5",
    "stylelint": "^12.0.0",
    "stylelint-scss": "^3.13.0",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  }
}
