{
  "name": "humsci_basic",
  "version": "1.0.0",
  "description": "Humsci Basic",
  "main": "Gruntfile.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "backstop:init": "node ./backstop/generate-backstop.js",
    "backstop:reference": "backstop reference --config='./backstop/backstop.json'",
    "backstop:test": "backstop test --config='./backstop/backstop.json'",
    "build:sass": "grunt compile",
    "build:ckeditor": "grunt ckeditor",
    "build:js": "webpack",
    "build": "npm run build:sass && npm run build:ckeditor && npm run build:js",
    "lint:sass": "stylelint 'src/scss/**/*.scss' --config ../../../../.stylelintrc",
    "test:sass": "mocha 'tests/true-sass-spec.js'",
    "watch:sass": "grunt watch",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "watch:js": "webpack --watch",
    "watch": "npm run watch:sass & npm run watch:js",
    "test": "npm run lint:sass && npm run test:sass",
    "visreg:colorful": "percy snapshot tests/percy/.snapshots-colorful.js",
    "visreg:traditional": "percy snapshot tests/percy/.snapshots-traditional.js",
    "visreg": "percy snapshot tests/percy/.snapshots-all.js",
    "copy": "node ./tasks/copy.js"
  },
  "license": "MIT",
  "dependencies": {
    "bourbon": "^5.1.0",
    "decanter": "^6.2.19",
    "dotenv": "^8.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@percy/cli": "^1.28.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^9.1.2",
    "backstopjs": "^6.1.4",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-webpack-plugin": "^3.2.0",
    "grunt": "^1.6.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-dart-sass": "^2.0.1",
    "grunt-import": "^1.0.3",
    "grunt-postcss": "^0.9.0",
    "grunt-run": "^0.8.1",
    "grunt-sass-tilde-importer": "^1.0.2",
    "mocha": "^10.2.0",
    "sass": "^1.58.0",
    "sass-true": "^7.0.0",
    "shelljs": "^0.8.5",
    "stylelint": "^15.1.0",
    "stylelint-scss": "^4.4.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
