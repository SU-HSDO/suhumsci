{#
/**
 * This Caption Credit template overrides the standard media.html.twig file in Stable theme.
 */
#}
{%
  set classes = [
    'hb-media-embed',
  ]
%}

<article{{ attributes.addClass(classes) }}>
  {{ title_suffix.contextual_links }}
  {% if content %}
    <div class='hb-media-embed__twitter'>
      {{ content }}
    </div>
  {% endif %}
</article>
